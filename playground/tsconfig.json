{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "resolvePackageJsonExports": true,
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "typeRoots": [
      "./types"
    ],
    "jsxImportSource": "react",
    "esModuleInterop": true
  },
  "exclude": [
    "dist"
  ]
}